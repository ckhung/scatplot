{
  "source": {
    "csv": "elec20/leg_at_large.csv",
    "textcols": ["縣市"],
    "pkey": "縣市",
    "keep": [
      "選舉人數>150000",
      "有效票比例>70"
    ]
  },
  "plotly": {
    "layout": {
      "title": {
	"text": "兩政黨各縣市得票比例對照圖",
	"font": {
	  "size": 30,
	  "color": "#000000"
	}
      },
      "font": {
        "size": 16,
        "color": "#0000ff"
      },
      "xaxis": {
	"title": {
	  "font": {
	    "size": 20,
	    "color": "#000000"
	  }
	}
      },
      "yaxis": {
	"title": {
	  "font": {
	    "size": 20,
	    "color": "#000000"
	  }
	}
      }
    },
    "maintrace": {
      "font": {
        "size": 16,
        "color": "#008"
      },
      "xaxis": { "expr": "中國國民黨" },
      "yaxis": { "expr": "民主進步黨" },
      "size": { "expr": "sqrt(選舉人數)*0.05" },
      "color": {
        "default": "#008"
      },
      "maintext": "縣市",
      "hovertext": "縣市"
    }
  }
}
