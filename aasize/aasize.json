{
  "source": {
    "csv": "aasize/aasize.csv",
    "textcols": ["Name", "FullName", "Type"],
    "pkey": "FullName"
  },
  "plotly": {
    "layout": {
      "title": "sizes of android apps",
      "xaxis": { "type": "log" },
      "yaxis": { "type": "log" }
    },
    "maintrace": {
      "xaxis": { "expr": "App" },
      "yaxis": { "expr": "Data" },
      "size": { "expr": "sqrt(sqrt(Cache))/2" },
      "maintext": "Name",
      "hovertext": "FullName",
      "color": {
        "default": "#fff",
        "colname": "Type",
        "palette": {
	  "fs": "#8f8",
	  "msg": "#a8f"
        }
      }
    }
  }
}
